<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="edit-button.png" />
    <None Remove="Images\backArrow.png" />
    <None Remove="Images\plus.png" />
    <None Remove="plus.png" />
    <None Remove="remove.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\backArrow.png" />
    <Resource Include="Images\plus.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Npgsql.EntityFrameworkCore.PostgreSQL" Version="3.1.11" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="edit-button.png" />
    <Resource Include="plus.png" />
    <Resource Include="remove.png" />
  </ItemGroup>

</Project>
